package com.tipdm.framework.controller.dmserver.entity;
import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel
public class Task {
    @ApiModelProperty(value = "任务组名称")
    private String taskGroup;

    @ApiModelProperty(value = "任务名称")
    private String name;

    @ApiModelProperty(value = "时间表达式(比如：0 0/2 * * * ?)")
    private String expression;

    @ApiModelProperty(value = "任务描述")
    private String description;

    @ApiModelProperty(value = "具体依赖已存在任务ID")
    private Long dependentJobId;

    @ApiModelProperty(value = "业务类别")
    private Type typeBean = Type.ENGINEERING;
    public enum Type{
        ENGINEERING,// 工程
        DATASOURCE;//数据源
    }
    @ApiModelProperty(value = "工程ID")
    private Long project;

    @ApiModelProperty(value = "工程名称 ")
    private String projectName;

    @ApiModelProperty(value = "数据源表名")
    private String dataSource;

    @ApiModelProperty(value = "是否有依赖")
    private boolean hasDepend = false;

    public String getProjectName() {
        return projectName;
    }

    public void setProjectName(String projectName) {
        this.projectName = projectName;
    }

    public boolean isHasDepend() {
        return hasDepend;
    }

    public void setHasDepend(boolean hasDepend) {
        this.hasDepend = hasDepend;
    }

    public Long getProject() {
        return project;
    }

    public void setProject(Long project) {
        this.project = project;
    }

    public String getDataSource() {
        return dataSource;
    }

    public void setDataSource(String dataSource) {
        this.dataSource = dataSource;
    }

    public Type getTypeBean() {
        return typeBean;
    }

    public void setTypeBean(Type typeBean) {
        this.typeBean = typeBean;
    }

    public String getTaskGroup() {
        return taskGroup;
    }

    public void setTaskGroup(String taskGroup) {
        this.taskGroup = taskGroup;
    }

    public Long getDependentJobId() {
        return dependentJobId;
    }

    public void setDependentJobId(Long dependentJobId) {
        this.dependentJobId = dependentJobId;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getExpression() {
        return expression;
    }

    public void setExpression(String expression) {
        this.expression = expression;
    }
}
