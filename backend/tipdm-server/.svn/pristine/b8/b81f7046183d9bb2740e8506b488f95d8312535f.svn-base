package com.tipdm.framework.model.dmserver;

import javax.persistence.*;
import java.io.Serializable;

/**
 * Created by zhoulong on 2017/3/16.
 * E-mail:zhoulong8513@gmail.com
 */
@Entity
@Table(name = "dm_shared_users")
public class SharedUsers implements Serializable {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    @Column(name = "user_id")
    private Long userId;

    @Column(name = "shared_object_id")
    private Long sharedObjectId;

    @Column(name = "object_type")
    @Enumerated()
    private ObjectType objectType;

    public SharedUsers(){

    }

    public SharedUsers(Long userId, Long sharedObjectId){
        this.userId = userId;
        this.sharedObjectId = sharedObjectId;
    }
    public enum ObjectType{
        DATASOURCE,
        MODEL;

        public Integer getValue(){
            return this.ordinal();
        }
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    public Long getSharedObjectId() {
        return sharedObjectId;
    }

    public void setSharedObjectId(Long sharedObjectId) {
        this.sharedObjectId = sharedObjectId;
    }

    public ObjectType getObjectType() {
        return objectType;
    }

    public void setObjectType(ObjectType objectType) {
        this.objectType = objectType;
    }
}
