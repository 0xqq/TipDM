package com.tipdm.framework.dmserver.websocket.workflow;

import com.tipdm.framework.dmserver.core.task.schedule.JobState;

/**
 * Created by TipDM on 2017/1/18.
 * E-mail:devp@tipdm.com
 */
public class WorkFlowMessage extends Message{

    public WorkFlowMessage(){
        super(Category.WORKFLOW);
    }

    private Type type = Type.NODE;

    private JobState state = JobState.INIT;

    public JobState getState() {
        return state;
    }

    public void setState(JobState state) {
        this.state = state;
    }

    public Type getType() {
        return type;
    }

    public void setType(Type type) {
        this.type = type;
    }

}

