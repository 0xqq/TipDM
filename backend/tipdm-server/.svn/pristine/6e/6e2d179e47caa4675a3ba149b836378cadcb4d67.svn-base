package com.tipdm.framework.controller.dmserver.entity.task;

import com.tipdm.framework.model.dmserver.DependentMode;
import com.tipdm.framework.model.dmserver.TaskType;
import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

/**
 * Created by zhoulong on 2018/1/23.
 * E-mail:zhoulong8513@gmail.com
 */
@ApiModel(value = "task")
public class AddModel {

    @ApiModelProperty(notes = "任务名称")
    private String name;

    @ApiModelProperty(notes = "时间表达式(比如：0 0/2 * * * ?)")
    private String expression;

    @ApiModelProperty(notes = "任务描述")
    private String description;

    @ApiModelProperty(notes = "依赖的任务ID")
    private Long dependentTaskId;

    @ApiModelProperty(notes = "任务依赖模式")
    private DependentMode dependentMode = DependentMode.AFTER;

    @ApiModelProperty(notes = "实例类别")
    private TaskType taskType = TaskType.ENGINEERING;

    @ApiModelProperty(notes = "要执行的实例ID")
    private Long instanceId;

    @ApiModelProperty(notes = "是否有依赖")
    private boolean hasDepend = false;

    public void setHasDepend(boolean hasDepend) {
        this.hasDepend = hasDepend;
    }

    public Long getInstanceId() {
        return instanceId;
    }

    public void setInstanceId(Long instanceId) {
        this.instanceId = instanceId;
    }

    public TaskType getTaskType() {
        return taskType;
    }

    public void setTaskType(TaskType taskType) {
        this.taskType = taskType;
    }

    public Long getDependentTaskId() {
        return dependentTaskId;
    }

    public void setDependentTaskId(Long dependentTaskId) {
        this.dependentTaskId = dependentTaskId;
    }

    public boolean getHasDepend() {
        return hasDepend;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getExpression() {
        return expression;
    }

    public void setExpression(String expression) {
        this.expression = expression;
    }

    public DependentMode getDependentMode() {
        return dependentMode;
    }

    public void setDependentMode(DependentMode dependentMode) {
        this.dependentMode = dependentMode;
    }
}
