<body>
    <div class="facets">

    </div>
<script>
    function setupVis() {
        $.when(protoAjax()).done(function (protoAjax) {
            var overview = $("#foelem")[0];
            overview.protoInput = protoAjax;
        });

        function protoAjax() {
            var url = window.location.href;
            var reg = new RegExp('id=([^&]*)(?:&)?');
            var id = url.match(reg) && url.match(reg)[1];
            var dataURL = "../" + id + ".html"
            return $.get({url: dataURL});
        }

    }
    $(document).on('click', 'a[href*=\\#]', function (event) {
        event.preventDefault();
        $('html, body').animate({
            scrollTop: $(this.hash).offset().top
        }, 500);
    });


</script>

<script>
    window.addEventListener('WebComponentsReady', function () {

        var link = document.createElement("link");
        link.rel = "import";
        link.href = "./facets.html";
        link.onload = function () {
            var overview = document.createElement("facets-overview");
            overview.id = "foelem";
            $(".facets")[0].appendChild(overview);
            setupVis();
        };
        document.head.appendChild(link);
    });


</script>


</body>

