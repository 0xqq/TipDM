package com.tipdm.framework.model.dmserver;

import java.util.Map;

/**
 * Interpreter rest api request message
 * 
 */
public class Interpreter {

	String id;
	String name;
	String group;
	Map<String, String> properties;
	Map<String, String> interpreterGroup;
	Map<String, String> option;
	Map<String, String> dependencies;

	public Interpreter() {

	}

	public String getName() {
		return name;
	}

	public String getGroup() {
		return group;
	}

	public Map<String, String> getProperties() {
		return properties;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public Map<String, String> getInterpreterGroup() {
		return interpreterGroup;
	}

	public Map<String, String> getDependencies() {
		return dependencies;
	}

	public void setDependencies(Map<String, String> dependencies) {
		this.dependencies = dependencies;
	}

	public Map<String, String> getOption() {
		return option;
	}

	public void setOption(Map<String, String> option) {
		this.option = option;
	}

	public void setInterpreterGroup(Map<String, String> interpreterGroup) {
		this.interpreterGroup = interpreterGroup;
	}
}
