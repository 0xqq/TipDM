package com.tipdm.framework.controller.dmserver.entity.datavisual;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

/**
 * Created by zhoulong on 2017/8/23.
 * E-mail:zhoulong8513@gmail.com
 */
@ApiModel
public class Value {

    @ApiModelProperty(name = "field", required = true, notes = "字段名", example = "field1", position = 1)
    private String field;

    @ApiModelProperty(name = "method", required = true, notes = "聚合函数名", example = "sum", position = 2)
    private AggregateFun method = AggregateFun.count;

    public String getField() {
        return field;
    }

    public void setField(String field) {
        this.field = field;
    }

    public AggregateFun getMethod() {
        return method;
    }

    public void setMethod(AggregateFun method) {
        this.method = method;
    }

    @Override
    public String toString() {
        String m = method.name();
        return m + "(" + field + ") as " + field + "_" + m;
    }
}
